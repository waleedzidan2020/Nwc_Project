@model SubscriberEditViewModel






<div class="container w-50 float-end ">
    <form asp-action="Add" asp-controller="Subscriber" method="post">
        <div class="form-group ">
            <label class=" m-2 float-end" asp-for="Id">رقم الهوية</label>
            <input type="text" class="form-control" asp-for="Id">
            <span asp-validation-for="Id" class="text-danger"></span>

        </div>
        <div class="form-group">
            <label class=" m-2 float-end" asp-for="Name">الاسم</label>
            <input type="text" class="form-control" asp-for="Name">
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class=" m-2 float-end" asp-for="City">المدينة</label>
            <input type="text" class="form-control" asp-for="City">
            <span asp-validation-for="City" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class="m-2 float-end" asp-for="Area">الحي</label>
            <input type="text" class="form-control" asp-for="Area">
            <span asp-validation-for="Area" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class=" m-2 float-end" asp-for="Mobile">رقم الجوال</label>
            <input type="text" class="form-control" asp-for="Mobile">
            <span asp-validation-for="Mobile" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-primary m-5">حفظ</button>
    </form>
</div>